	<array>
		<string>googlechromes</string>
		<string>comgooglemaps</string>
		<string>comgooglemaps-x-callback</string>
	</array>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>NSAppleMusicUsageDescription</key>
	<string>GHMC Officer app requires access to your  gallery to get the grievance information</string>
	<key>NSCameraUsageDescription</key>
	<string>GHMC Officer app requires access to your camera to capture grievance complaints</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>GHMC Officer app requires access to your Location to identify the complaints raised by Officer_Citezen</string>
